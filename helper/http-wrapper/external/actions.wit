package betty-blocks:types;

interface actions {
   type json-string = string;

   record payload {
      input: json-string,
   }

   record input {
      action-id: string,
      payload: payload,
   }

   record output {
      %result: json-string,
   }

   call: func(input: input) -> result<output, string>;
   health: func() -> result<string, string>;
}